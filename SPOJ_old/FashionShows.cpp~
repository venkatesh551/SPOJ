#include <iostream>
#include <algorithm>

using namespace std;

void read_andSort(int *a, int n)
{
	for (int i = 0; i < n; ++i)
	{
		cin >> a[i];
	}
	sort(a, a+n);
}

int main()
{
	int T;
	
	cin >> T;
	while (T--)
	{
		int n;
		const int SIZE = 1e3;
		int A[SIZE], B[SIZE];
		cin >> n;
		read_andSort(A, n);
		read_andSort(B, n);
		int sum = 0;
		for (int i = 0; i < n; ++i)
		{
			sum += a[i] * b[i];
		}
		cout << sum << endl;
	}
	return 0;
}
